{
    "name": "dohnall/nais",
    "type": "project",
    "description": "The NAIS project.",
    "license": "MIT",
    "require": {
        "laravel/laravel": "*",
        "dohnall/copy-files": "*"
    },
    "require-dev": {

    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "Dohnall\\CopyFiles\\CopyFiles::run",
            "@composer install"
        ]
    }
}
